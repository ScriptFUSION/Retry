{
  "name": "scriptfusion/retry",
  "description": "Retries failing operations.",
  "license": "MIT",
  "authors": [
    {
      "name": "Bilge",
      "email": "bilge@scriptfusion.com"
    },
    {
      "name": "Igor Wiedler",
      "email": "igor@wiedler.ch"
    }
  ],
  "require": {
    "php": "^8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "files": [
      "src/retry.php",
      "src/FailingTooHardException.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ScriptFUSIONTest\\Retry\\": "test"
    }
  },
  "scripts": {
    "test": "phpunit -c test"
  },
  "config": {
    "sort-packages": true
  }
}
